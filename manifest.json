{
  "name": "GSPro",
  "description" : "Base Level Extension",
  "version": "0.1",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "webNavigation",
    "*://gossluzhba.gov.ru/*",
    "*://mail.gossluzhba.gov.ru/owa/rezerv@gossluzhba.gov.ru/#path=/mail"
  ],
  "content_scripts": [
    {
      "matches": ["*://mail.gossluzhba.gov.ru/owa/rezerv@gossluzhba.gov.ru/*"],
      "run_at": "document_idle",
      "all_frames": true,
      "js": ["contentScript.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "web_accessible_resources": ["content.js"]
}
